    "jenkinsLV": {
      "command": "wdio wdio.conf.js",
      "description": "Jenkins Testing Capabilities Load - Large View",
      "env": {
        "CAPABILITY": "jenkinsLV",
        "TAG": "@regression",
        "VIEW": "LV",
        "TESTENV": "test",
        "DRIVER": "remote",
        "PARALLEL": "5",
        "JENKINS": "enabled"
      }
    },
    "jenkinsSV": {
      "command": "wdio wdio.conf.js",
